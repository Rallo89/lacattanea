r>\n    </Navbar>\n  );\n  \n}","/Users/nicolotorreggiani/Documents/GitHub/lacattanea/src/Components/Signup.js",["62"],"import React, { useRef, useState } from 'react'\nimport { Form, Button, Card, Alert } from 'react-bootstrap'\nimport { useAuth } from '../context/AuthContext'\n\nexport default function Signup(){\n    const emailRef = useRef();\n    const passwordRef = useRef();\n    const passwordConfirmRef = useRef();\n    const { signup, currentUser } = useAuth()\n    const [error, setError] = useState('')\n    const [loading, setLoading] = useState(false)\n\n    async function handleSubmit(e){\n        e.preventDefault()\n\n        if(passwordConfirmRef.current.value !== passwordRef.current.value){\n            return setError('Le password non coincidono')\n        }\n\n        try{\n            setError('')\n            //Per evitare la creazione di account multipli\n            setLoading(true)\n            console(signup(emailRef.current.value, passwordRef.current.value))\n        } catch {\n            setError('Errore durante la creazione dell\\'account du Firebase')\n        }\n        setError('')\n        setLoading(false)\n    }\n\n    return(\n        <Card>\n            <Card.Body>\n                <h2 className=\"text-center mb-4\">Sign Up</h2>\n                {error && <Alert variant='danger'>{error}</Alert>}\n                <Form onSubmit={handleSubmit}>\n                    <Form.Group id=\"email\">\n                        <Form.Label>Email</Form.Label>\n                        <Form.Control type=\"email\" ref={emailRef} required />\n                    </Form.Group>\n                    <Form.Group id=\"password\">\n                        <Form.Label>Password</Form.Label>\n                        <Form.Control type=\"password\" ref={passwordRef} required />\n                    </Form.Group>\n                    <Form.Group id=\"password-confirm\">\n                        <Form.Label>Password Confirmation</Form.Label>\n                        <Form.Control type=\"password\" ref={passwordConfirmRef} required />\n                    </Form.Group>\n                    <Button disabled={loading} className=\"w-100\" type=\"submit\">\n                        Sign Up\n                    </Button>\n                </Form>\n            </Card.Body>\n        </Card>\n    );\n}","/Users/nicolotorreggiani/Documents/GitHub/lacattanea/src/context/AuthContext.js",[],"/Users/nicolotorreggiani/Documents/GitHub/lacattanea/src/firebase.js",[],{"ruleId":"63","severity":1,"message":"64","line":1,"column":17,"nodeType":"65","messageId":"66","endLine":1,"endColumn":26},{"ruleId":"63","severity":1,"message":"67","line":5,"column":8,"nodeType":"65","messageId":"66","endLine":5,"endColumn":14},{"ruleId":"63","severity":1,"message":"68","line":12,"column":10,"nodeType":"65","messageId":"66","endLine":12,"endColumn":14},{"ruleId":"63","severity":1,"message":"69","line":17,"column":9,"nodeType":"65","messageId":"66","endLine":17,"endColumn":12},{"ruleId":"63","severity":1,"message":"70","line":18,"column":9,"nodeType":"65","messageId":"66","endLine":18,"endColumn":13},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"63","severity":1,"message":"75","line":3,"column":8,"nodeType":"65","messageId":"66","endLine":3,"endColumn":14},{"ruleId":"63","severity":1,"message":"76","line":7,"column":27,"nodeType":"65","messageId":"66","endLine":7,"endColumn":33},{"ruleId":"63","severity":1,"message":"77","line":7,"column":35,"nodeType":"65","messageId":"66","endLine":7,"endColumn":41},{"ruleId":"63","severity":1,"message":"78","line":7,"column":43,"nodeType":"65","messageId":"66","endLine":7,"endColumn":48},{"ruleId":"63","severity":1,"message":"79","line":9,"column":8,"nodeType":"65","messageId":"66","endLine":9,"endColumn":15},{"ruleId":"63","severity":1,"message":"80","line":9,"column":21,"nodeType":"65","messageId":"66","endLine":9,"endColumn":32},"no-unused-vars","'Component' is defined but never used.","Id